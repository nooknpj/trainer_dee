            "properties": {
                "firstName": {
                    "type": "string",
                    "example": "John"
                },
                "lastName": {
                    "type": "string",
                    "example": "Calvin"
                },
                "gender": {
                    "type": "string",
                    "example": "M"
                },
                "address": {
                    "type": "string",
                    "example": "205-1017, Ikanikeisaiganaibaai, Tsurui-mura Akan-gun, Hokkaido"
                },
                "telNo": {
                    "type": "string",
                    "example": "0589665489"
                },
